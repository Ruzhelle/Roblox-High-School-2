local player = game.Players.LocalPlayer

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "R. Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
    Name = "Club Red",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "Welcome!",
    Content = "DM Witchyy#6039 for Issues!",
    Image = "rbxassetid://4483345998",
    Time = 5
})

local Section = Tab:AddSection({
    Name = "Club Red"
})

Tab:AddButton({
    Name = "Red Spice",
    Callback = function()
           local args = {
    [1] = "RedSpice"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Jobs"):WaitForChild("ClubRed"):WaitForChild("MakePotion"):FireServer(unpack(args))
      end    
})

